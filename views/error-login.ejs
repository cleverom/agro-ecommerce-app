	<!--=============================================
	=            Login register page content         =
	=============================================-->

	<div class="page-content mb-50">
		<div class="container">
			<div class="row">
				<div class="col-sm-12 col-md-12 col-xs-12 col-lg-6 mb-30">
					<!-- Login Form s-->
					<form id="loginForm" action="/login-register"  method="post">

						<div class="login-form">
							<h4 class="login-title">Login</h4>
              <p style="color: red"><%= fieldError %></p>

							<div class="row">
								<div class="col-md-12 col-12 mb-20">
									<label>Email Address*</label>
									<input name="email" class="mb-0" type="email" placeholder="Email Address" value="<%= typeof email !== 'undefined' ? email : '' %>" Required>
                  <p style="color: red"><%= emailError %></p>
								</div>
								<div class="col-12 mb-20">
									<label>Password</label>
									<input name="password" class="mb-0" type="password" placeholder="Password" value="<%= typeof password !== 'undefined' ? password : '' %>">
                  <p style="color: red"><%= passwordError %></p>
								</div>

								<div class="col-md-12">
									<input type="submit" class="register-button mt-0">
								</div>

							</div>
						</div>

					</form>
				</div>
				<div class="col-sm-12 col-md-12 col-xs-12 col-lg-6">
					<form  id="login-form" method="POST" action="/reg">

						<div class="login-form">
							<h4 class="login-title">Register</h4>
              <div class="form-message form-message-error"></div>


							<div class="row">
								<div class="col-md-6 col-12 mb-20">
									<label>First Name</label>
									<input name="firstName" class="mb-0" type="text" placeholder="First Name" Required>
								</div>

								<div class="col-md-6 col-12 mb-20">
									<label>Last Name</label>
									<input name="lastName" class="mb-0" type="text" placeholder="Last Name" Required>
								</div>

								<div class="col-md-12 mb-20">
									<label>Email Address*</label>
									<input name="email" class="mb-0" type="email" placeholder="Email Address" Required>

								</div>

								<div class="col-md-6 mb-20">
									<label>Password</label>
									<input name="password" class="mb-0" type="password" placeholder="Password" Required>

								</div>

								<!-- <div class="col-md-6 mb-20">
									<label>Confirm Password</label>
									<input name="firstName" class="mb-0" type="password" placeholder="Confirm Password">
								</div> -->

								<div class="col-12">
									<button type="submit" value="register" class="register-button mt-0">Register</button>
								</div>
							</div>
						</div>

					</form>
				</div>
			</div>
		</div>
	</div>

